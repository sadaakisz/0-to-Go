	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"Print to value and type": {
		"prefix": "pr",
		"body": [
			"fmt.Printf(\"%v, %T\\n\", $0)"
		],
		"description": "Log output and type to console"
	},
	"Print to console": {
		"prefix": "pl",
		"body": [
			"fmt.Println($0)"
		],
		"description": "Log output and type to console"
	},
	"Initialize Go": {
		"prefix": "pkg",
		"body": [
			"package main\n",
			"import \"fmt\"\n",
			"func main() {",
			"\t$0",
			"}"
		],
		"description": "Classic inizialize"
	}
}